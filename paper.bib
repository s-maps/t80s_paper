  date-modified	= {2015-04-27 15:21:14 +0000},
  doi		= {10.1046/j.1365-8711.2003.06231.x},
  eprint	= {astro-ph/0209432},
  journal	= {\mnras},
  keywords	= {turbulence, atmospheric effects, instrumentation: adaptive
		  optics, site testing},
  mnrascitekey	= tokovinin.baumont.vasquez.2003,
  month		= mar,
